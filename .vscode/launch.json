    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "type": "lldb",
            "request": "launch",
            "target": "./target/debug/proxy",
            "program": "${workspaceRoot}/target/debug/proxy",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText",
            "args": "-s stdio: -s b64:fw -s ws://api.kamran.org:6666",
            // "args": "-s ws-l://127.0.0.1:6666 -s b64:fw -s stdio:",
            "env": {
                "RUST_BACKTRACE": "1",
            },
        }
    ]
}